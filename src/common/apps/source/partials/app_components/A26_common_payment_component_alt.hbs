{{#if Alt_Payment_Methods_Radio_Buttons}}<!-- Begin Radio buttons --><fieldset class="ups-form_group">	<legend class="ups-form_label"> {{this.radio_button_title}}</legend>	<!-- Radio Button #1 -->	{{#each radio_button}}	<div class="ups-buttonList_wrapper ups-input_wrapper">		<input id="{{this.id}}"             class="ups-radio-custom"            type="radio"             name="{{this.name}}"            value="{{this.value}}">		<label for="{{this.for}}" class="ups-radio-custom-label"> {{this.copy}}</label>		{{#if radio_button_image}}<img src="{{this.radio_button_image}}" height="20px" width="40px">{{/if}}	</div>	{{/each}}</fieldset><!-- End Radio buttons --> {{/if}}{{#if pay_at_store}}<h3>Pay At Store</h3><p>Pay for this shipment when you drop off your packages at a UPS Access Point location. Bring a copy of your shipping ticket ID or show the ID barcode on your mobile device. Your label will be printed at the store when your package is accepted.</p><p>We'll send a copy of the shipping ticket and receipt to the email address below.</p>{{/if}}{{#if paypal_transferred}}<h3>PayPal</h3><p>You will be transferred to [PayPal] to complete this transaction.</p>{{/if}}{{#if paypal_rest_easy}}<h3>PayPal</h3><p>You will be transferred to [PayPal] to complete this transaction.</p>{{/if}}{{#if select_paypal_acct_radio_buttons}}<!-- Begin Radio buttons --><fieldset class="ups-form_group">	<legend class="ups-form_label"> {{this.radio_button_title2}}</legend>	<!-- Radio Button #1 -->	{{#each radio_button2}}	<div class="ups-buttonList_wrapper ups-input_wrapper">		<input id="{{this.id2}}"             class="ups-radio-custom"            type="radio"             name="{{this.name2}}"            value="{{this.value2}}">		<label for="{{this.for2}}" class="ups-radio-custom-label"> {{this.copy2}}</label>	</div>	{{/each}}</fieldset><!-- End Radio buttons --><div class="ups-group">    <button type="button" class="ups-link">+ Add a PayPal Account</button></div>{{/if}}{{#if choose_payment_card_radio_buttons}}<!-- Begin Radio buttons --><fieldset class="ups-form_group">	<legend class="ups-form_label"> {{this.radio_button_title}}</legend>	<!-- Radio Button #1 -->	{{#each radio_button}}	<div class="ups-buttonList_wrapper ups-input_wrapper">		<input id="{{this.id}}"             class="ups-radio-custom"            type="radio"             name="{{this.name}}"            value="{{this.value}}">		<label for="{{this.for}}" class="ups-radio-custom-label">{{#if radio_button_image}}<img src="{{this.radio_button_image}}" height="20px" width="40px">{{/if}} {{this.copy}}</label>	</div>	{{/each}}</fieldset><!-- End Radio buttons --><div class="ups-group">    <button type="button" class="ups-link" id="addNewCardLink">+ Add new card</button> </div>{{/if}}{{#if email_language_inputs}}<div class="row">	<div class="col-md-6">		<div class="ups-form_group">			<label for="ups-email" class="ups-form_label">Email</label>			<div class="ups-text_wrapper ups-input_wrapper">				<input type="text" name="ups-email" class="ups-form_input" id="ups-email">			</div>		</div>	</div>	<div class="col-md-6">		<div class="ups-form_group">			<label for="ups-notification_lang" class="ups-form_label">Notification Language</label>			<div class="ups-dropdown_wrapper ups-input_wrapper">				<select type="text" name="ups-notification_lang" class="ups-dropdown" id="ups-notification_lang">					<option selected="selected">{language from shipper AP notification}</option>					<option value="">sample</option>					<option value="">sample</option>					<option value="">sample</option>				</select>			</div>		</div>	</div></div>{{/if}}{{#if send_a_copy_cb}}<!-- Begin Checkbox (Send a copy of shipping invoice to my address as well) --><div class="ups-form_group">    <div class="ups-checkbox ups-input_wrapper">        <input check-verify id="ups-sendACopyCB"            class="ups-checkbox-custom ups-ng-pristine data-ng-untouched data-ng-valid"            type="checkbox" name="makeDefault" data-ng-pattern=""            data-ng-model="form.makeDefault"            data-ng-model-options="{updateOn: 'blur'}">        <label for="ups-sendACopyCB" class="ups-checkbox-custom-label">Send a copy of shipping invoice to my address as well.</label>    </div></div><!-- End Checkbox (Send a copy of shipping invoice to my address as well) --> <p><strong>{AP Notification shipper's email address}</strong></p>{{/if}}{{#if billing_address_radio_buttons}}<!-- Begin Billing address Radio buttons --><fieldset class="ups-form_group">	<legend class="ups-form_label">Billing Address</legend>	<div class="ups-buttonList_wrapper ups-input_wrapper">		<input id="ups-sameBillingAddress"             class="ups-radio-custom"            type="radio"             name="ups-sameBillingAddress"            value="SameAdd">		<label for="ups-sameBillingAddress" class="ups-radio-custom-label"> {2010 Warsaw Rd., Roswell, GA 30076}</label>	</div>	<div class="ups-buttonList_wrapper ups-input_wrapper">		<input id="ups-diffBillingAddress"             class="ups-radio-custom"            type="radio"             name="ups-sameBillingAddress"            value="diffAdd">		<label for="ups-diffBillingAddress" class="ups-radio-custom-label"> Enter a different address.</label>	</div>	<!-- ups-form_group ups-form_required --></fieldset><!-- End Billing address Radio buttons --> {{/if}}{{#if tax_id_input}}<div class="row">	<div class="col-md-6">		<div class="ups-form_group">			<label for="enter_here" class="ups-form_label">Tax ID for printed invoice</label>			<p class="ups-form_subheader">Optional</p>			<div class="ups-text_wrapper ups-input_wrapper">				<input type="text" name="" class="ups-form_input" id="" maxlength="">			</div>		</div>	</div></div>{{/if}}{{#if save_this_payment_cb}}<!-- Begin Checkbox (Send a copy of shipping invoice to my address as well) --><div class="ups-form_group">    <div class="ups-checkbox ups-input_wrapper">        <input check-verify id="ups-saveThisPaymentCB"            class="ups-checkbox-custom ups-ng-pristine data-ng-untouched data-ng-valid"            type="checkbox" name="ups-saveThisPaymentCB" data-ng-pattern=""            data-ng-model="form.makeDefault"            data-ng-model-options="{updateOn: 'blur'}">        <label for="ups-saveThisPaymentCB" class="ups-checkbox-custom-label">Save this information to my profile.</label>    </div></div><!-- End Checkbox (Send a copy of shipping invoice to my address as well) --> {{/if}}