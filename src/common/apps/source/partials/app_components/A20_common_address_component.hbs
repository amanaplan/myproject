<!-- ************************************************************************************ --><!-- A20 Common Address Component                                                         --><!-- ************************************************************************************ -->{{!--********************************************************************************A20 Common Address Component - Data********************************************************************************Available Variations via class names:    US Skin        used in US region            Canada Skin        used in Canada regionConfigs via DATA attributes:    DATA-ATTRIBUTE            TYPE          REQUIRED          EXAMPLE    country_option             = array      / YES           / List of countries or territories    mobile_option              = array      / YES           / List of phone types    provence           		   = string     / YES           / Provence    provence_option            = array      / YES           / List of provences    postal           		   = string     / YES           / Postal Code********************************************************************************--}} <!-- /////////////////////////////// --><!-- //// Begin CAC             //// --><!-- /////////////////////////////// --><!-- documentation ../source/module/A20_common_address_component.hbs -->{{#if nickname_input}}<!-- Begin Nickname --><div class="ups-form_group">    <label for="ups-nick_name_input" class="ups-form_label">Nickname</label>    <div class="ups-text_wrapper ups-input_wrapper">        <input type="text"         	name="nickName"             class="ups-form_input"            id="ups-nick_name_input"             value=""            maxlength="35">    </div>    <!-- ups-text_wrapper ups-input_wrapper --></div><!-- End Nickname -->{{/if}}{{#if country_sb}}<!-- Begin Country selectbox --><div class="ups-form_group ups-form_required">    <label for="ups-country" class="ups-form_label">    	Country        <span aria-hidden="true" class="ups-required_indicator"></span>    </label>    <div class="ups-dropdown_wrapper ups-text_wrapper ups-input_wrapper">         <select name="ups-country_select"          	id="ups-country_select"             class="ups-dropdown{{#if error}} ng-invalid ng-dirty{{/if}}"            {{#if error}}aria-describedby="ups-country-error"{{/if}}>                  		{{#each country_option}}                  <option value="{{this.value}}">{{this.copy}}</option>                  		{{/each}}            </select>		{{#if non_error}}		<div class="ups-check ups-active">			<span class="icon selectbox ups-icon-check" aria-hidden="true"></span>			<span class="ups-readerTxt">Check</span>		</div>		{{/if}}	</div>	{{#if error}} 	<span class="ups-icons-error" id="ups-country-error" role="alert"> 		<span class="ups-invalid_color"> 			<span class="icon ups-icon-exclamation ups-error_exclamation"></span> 			<span class="ups-readerTxt">Invalid</span> <span>Country field error.</span> 		</span> 	</span> 	{{/if}}     <!-- ups-dropdown_wrapper ups-text_wrapper ups-input_wrapper --></div><!-- End Country selectbox -->{{/if}}{{#if country_prepopulated}}<!-- Begin Country prepopulated --><div class="ups-group">    <p class="ups-group_label">Country</p>    <p class="ups-group_value">UNITED STATES</p></div><!-- End Country prepopulated -->{{/if}}{{#if contact_input}}<!-- Begin Name --><div class="row">	<div class="col-md-6">{{/if}}{{#if name_input}}		<div class="ups-form_group ups-form_required">			<label for="ups-name_input" class="ups-form_label">				Name				<span aria-hidden="true" class="ups-required_indicator"></span>			</label>			<div class="ups-text_wrapper ups-input_wrapper">				<input type="text" 					name="name"					class="ups-form_input{{#if error}} ng-invalid ng-dirty{{/if}}" 					id="ups-name_input" 					value=""					required 					maxlength="35"					{{#if error}}aria-describedby="ups-name-error"{{/if}}					aria-required="true"> 				{{#if non_error}}				<div class="ups-check ups-active">					<span class="icon ups-icon-check" aria-hidden="true"></span>					<span class="ups-readerTxt">Check</span>				</div>				{{/if}}			</div>			{{#if error}}			<span class="ups-icons-error" id="ups-name-error" role="alert">				<span class="ups-invalid_color">					<span class="icon ups-icon-exclamation ups-error_exclamation"></span>					<span class="ups-readerTxt">Invalid</span>					<span>Name error.</span>				</span>			</span>			{{/if}}		</div>{{/if}}{{#if contact_input}}	</div>	<!-- End Name -->		<!-- Begin Contact -->	<div class="col-md-6">		<div class="ups-form_group">			<label for="ups-default_address" class="ups-form_label">Contact</label>			<p id="contactHelperTxt" class="ups-form_subheader">{{ups_placeholder.contact}}</p>			<div class="ups-text_wrapper ups-input_wrapper">				<input type="text" 					name="contact" 					class="ups-form_input"					id="ups-default_address" 					maxlength="35"					aria-describedby="contactHelperTxt">			</div>			<!-- ups-text_wrapper ups-input_wrapper -->		</div>	</div>	<!-- End Contact --></div>{{/if}}{{#if address1_input}}<!-- Begin Address (street) --><div class="ups-form_group ups-form_required">    <label for="ups-address_1_input" class="ups-form_label">    	Address        <span aria-hidden="true" class="ups-required_indicator"></span>    </label>    <!--<p class="ups-form_subheader">{{ups_placeholder.address1}}</p>-->    <p id="streetHelpherTxt" class="ups-form_subheader ups-readerTxt">Street</p>    <div class="ups-text_wrapper ups-input_wrapper">        <input type="text"         	name="address1"            class="ups-form_input{{#if error}} ng-invalid ng-dirty{{/if}}"             id="ups-address_1_input"             maxlength="35"            required            aria-describedby="{{#if error}}ups-address-error {{/if}}streetHelperTxt"             aria-required="true"			placeholder="Street Address">		{{#if non_error}}		<div class="ups-check ups-active">			<span class="icon ups-icon-check" aria-hidden="true"></span>			<span class="ups-readerTxt">Check</span>		</div>		{{/if}}    </div>    {{#if error}}    <span class="ups-icons-error" id="ups-address-error" role="alert">        <span class="ups-invalid_color">            <span class="icon ups-icon-exclamation ups-error_exclamation"></span>            <span class="ups-readerTxt">Invalid</span>            <span>Street Address error.</span>        </span>    </span>    {{/if}}</div><!-- End Address (street) -->{{/if}}{{#if address2_input}}<!-- Begin Address (apt) --><div class="ups-form_group">    <label for="ups-address_2_input" class="ups-readerTxt">Apartment, Suite, Unit</label><!--    <p id="address2HelperTxt" class="ups-form_subheader" class="ups-readerTxt">Apartment, Suite, Unit</p>-->    <div class="ups-text_wrapper ups-input_wrapper">        <input type="text"         	name="address2"         	class="ups-form_input"            id="ups-address_2_input"             maxlength="35"            aria-describedby="address2HelperTxt"			placeholder="Apt.,Suite,Unit">    </div>    <!-- ups-text_wrapper ups-input_wrapper --></div><!-- End Address (apt) -->{{/if}}{{#if address3_input}}<!-- Begin Address (depot) --><div class="ups-form_group">    <label for="ups-address_3_input" class="ups-readerTxt">Department, Care of, etc.</label><!--    <p class="ups-form_subheader" class="ups-readerTxt">Department, Care of, etc.</p>-->    	<div class="ups-text_wrapper ups-input_wrapper">        <input type="text"         	name="address3"             class="ups-form_input"            id="ups-address_3_input"             maxlength="35"			placeholder="Dept.,c/o,etc">    </div>    <!-- ups-text_wrapper ups-input_wrapper --></div><!-- End Address (depot) -->{{/if}}{{#if city_input}}<!-- Begin City --><div class="ups-form_group ups-form_required">    <label for="ups-city" class="ups-form_label">    	City        <span aria-hidden="true" class="ups-required_indicator"></span>    </label>    <p id="cityHelperTxt" class="ups-form_subheader">{{ups_placeholder.city}}</p>    <div class="ups-text_wrapper ups-input_wrapper">        <input type="text"         	name="city"            class="ups-form_input{{#if error}} ng-invalid ng-dirty{{/if}}"             id="ups-city"             maxlength="35"             required            aria-describedby="{{#if error}}ups-city-error {{/if}}cityHelperTxt"             aria-required="true">		{{#if non_error}}		<div class="ups-check ups-active">			<span class="icon ups-icon-check" aria-hidden="true"></span>			<span class="ups-readerTxt">Check</span>		</div>		{{/if}}    </div>    {{#if error}}    <span class="ups-icons-error" id="ups-city-error" role="alert">        <span class="ups-invalid_color">            <span class="icon ups-icon-exclamation ups-error_exclamation"></span>            <span class="ups-readerTxt">Invalid</span>            <span>City error.</span>        </span>    </span>    {{/if}}</div><!-- End City -->{{/if}}<!-- Begin State/ZIP --><div class="row">    {{#if state_sb}}	<!-- Begin State -->    <div class="col-sm-7">        <div class="ups-form_group ups-form_required">            <label for="ups-state_select" class="ups-form_label">                {{provence}}                <span aria-hidden="true" class="ups-required_indicator"></span>            </label>            <div class="ups-dropdown_wrapper ups-input_wrapper">                <select name="provence_dropdown"                 	id="ups-provence_select"                     class="ups-dropdown{{#if error}} ng-invalid ng-dirty{{/if}}"                     required		            {{#if error}}aria-describedby="ups-provence-error"{{/if}}>                    <option selected="selected" value="" disabled="disabled">Select One</option>                                        {{#each provence_option}}                                        <option value="{{this.value}}">{{this.copy}}</option>                                        {{/each}}                                </select>				 {{#if non_error}}				<div class="ups-check ups-active">					<span class="icon selectbox ups-icon-check" aria-hidden="true"></span>					<span class="ups-readerTxt">Check</span>				</div>				{{/if}}            </div>            {{#if error}}            <span class="ups-icons-error" id="ups-provence-error" role="alert">                <span class="ups-invalid_color">                    <span class="icon ups-icon-exclamation ups-error_exclamation"></span>                    <span class="ups-readerTxt">Invalid</span>                    <span>State error.</span>                </span>            </span>            {{/if}}        </div>        <!-- ups-form_group ups-form_required -->    </div>	<!-- End State -->    {{/if}}    {{#if postal_input}}	<!-- Begin ZIP -->    <div class="col-sm-5">        <div class="ups-form_group ups-form_required">            <label for="ups-postal_input" class="ups-form_label">                {{postal}}                <span aria-hidden="true" class="ups-required_indicator"></span>            </label>            <div class="ups-text_wrapper ups-input_wrapper">                <input type="text"                 	class="ups-form_input{{#if error}} ng-invalid ng-dirty{{/if}}"                    id="ups-postal_input"                     maxlength="10"                     name="zipcode"                     required                    {{#if error}}aria-describedby="ups-postal-error"{{/if}}                     aria-required="true"> 					{{#if non_error}}					<div class="ups-check ups-active">						<span class="icon ups-icon-check" aria-hidden="true"></span>						<span class="ups-readerTxt">Check</span>					</div>					{{/if}}            </div>            {{#if error}}            <span class="ups-icons-error" id="ups-postal-error" role="alert">                <span class="ups-invalid_color">                    <span class="icon ups-icon-exclamation ups-error_exclamation"></span>                    <span class="ups-readerTxt">Invalid</span>                    <span>{{postal}} error.</span>                </span>            </span>            {{/if}}        </div>        <!-- ups-form_group ups-form_required -->    </div>	<!-- End ZIP -->    {{/if}}</div><!-- End State/ZIP -->{{#if res_address_cb}}<!-- Begin Checkbox (Res address) --><div class="ups-form_group ups-checkbox_group">    <div class="ups-checkbox ups-text_wrapper ups-input_wrapper">        <input check-verify         	type="checkbox"         	id="ups-residential_address"            class="ups-checkbox-custom">         <label for="ups-residential_address" class="ups-checkbox-custom-label">        	This is a residential address</label>    </div>    <!-- ups-checkbox ups-text_wrapper ups-input_wrapper --></div><!-- End Checkbox (Res address) -->{{/if}}<!-- Begin Phone/Ext --><div class="row">	{{#if phone_input}}    <!-- Begin Phone -->    <div class="col-sm-7">        <div class="ups-form_group">            <label for="ups-phone_input" class="ups-form_label">Phone</label>            <p id="phoneHelperTxt" class="ups-form_subheader ups-readerTxt">555-555-5555</p>            <div class="ups-text_wrapper ups-input_wrapper">                <input type="text"                 	name="phone"                    class="ups-form_input"                     id="ups-phone_input"                    maxlength="15"                    aria-describedby="phoneHelperTxt">            </div>            <!-- ups-text_wrapper ups-input_wrapper -->        </div>        <!-- ups-form_group -->    </div>    <!-- End Phone -->    {{/if}}	{{#if ext_input}}    <!-- Begin Ext -->    <div class="col-sm-5">        <div class="ups-form_group">            <label for="ups-extension_input">Ext.</label>            <p id="extHelperTxt" class="ups-form_subheader ups-readerTxt">Ext.</p>            <div class="ups-text_wrapper ups-input_wrapper">                <input type="text" name="ext" class="ups-form_input"                    id="ups-extension_input" data-ng-pattern="/^[0-9]*$/"                    maxlength="5" data-ng-model="formObj.ext" aria-describedby="extHelperTxt">            </div>            <!--ups-text_wrapper ups-input_wrapper -->        </div>        <!-- ups-form_group -->    </div>    <!-- Begin Ext -->    {{/if}}</div><!-- End Phone/Ext -->{{#if phone_type_sb}}<!-- Begin Phone Type --><div class="ups-form_group">    <!-- <label for="ups-extension_input" class="ups-form_label"></label> -->    <label for="ups-suffix_select" class="ups-no_label ups-readerTxt">Phone Type</label>    <div class="ups-dropdown_wrapper ups-input_wrapper">        <select name="select" id="ups-phoneType" class="ups-dropdown">            {{#each mobile_option}}            <option value="{{this.value}}">{{this.value}}</option>            {{/each}}        </select>    </div>    <!-- ups-dropdown_wrapper ups-text_wrapper ups-input_wrapper --></div><!-- End Phone Type -->{{/if}}{{#if fax_input}}<!-- Begin Fax --><div class="ups-form_group">    <label for="ups-fax" class="ups-form_label">Fax</label>    <p id="faxHelperTxt" class="ups-form_subheader ups-readerTxt">555-555-5555</p>    <div class="ups-text_wrapper ups-input_wrapper">        <input type="text"         	name="fax"             class="ups-form_input"             id="ups-fax"            value=""             maxlength="15"            aria-describedby="faxHelperTxt">    </div>    <!-- ups-text_wrapper ups-input_wrapper --></div><!-- End Fax -->{{/if}}{{#if email_input}}<!-- Begin Email --><div class="ups-form_group ups-form_required">    <label for="ups-email_input" class="ups-form_label">    	Email        <span aria-hidden="true" class="ups-required_indicator"></span>    </label>    <div class="ups-text_wrapper ups-input_wrapper">        <input type="email"         	name="email_input"             class="ups-form_input{{#if error}} ng-invalid ng-dirty{{/if}}"            id="ups-email_input"             value=""             maxlength="50"             required            {{#if error}}aria-describedBy="ups-email-error"{{/if}}             aria-required="true">			 {{#if non_error}}			<div class="ups-check ups-active">				<span class="icon ups-icon-check" aria-hidden="true"></span>				<span class="ups-readerTxt">Check</span>			</div>			{{/if}}    </div>     {{#if error}}    <span class="ups-icons-error" id="ups-email-error" role="alert">        <span class="ups-invalid_color">            <span class="icon ups-icon-exclamation ups-error_exclamation"></span>            <span class="ups-readerTxt">Invalid</span>            <span>Required Information error.</span>        </span>    </span>    {{/if}}    <!-- ups-text_wrapper ups-input_wrapper --></div><!-- End Email -->{{/if}}{{#if lacal_input}}<!-- Begin Location ID --><div class="ups-form_group ups-help_wrapper">    <label for="ups-location_id_input" class="ups-form_label">Location ID </label>     <span class="ups-help">         <a href="#" class="ups-help-position ups-help_link ups-form_link-noDecoration">            Help             <span class="ups-icon-questioncircle" aria-hidden="true"></span>        </a>         <span class="ups-help_tooltip ups-help_tooltip_topleft">                <button type="button" class="ups-help_close icon ">                    <span class="ups-icon-x ups-iconAlone" aria-hidden="true"></span>                    <span class="ups-readerTxt">Close</span>                </button>                 <span>Explanatory text lorem ipsum dolor sit amet.</span>        </span>    </span>    <p id="locationIdHelperTxt" class="ups-form_subheader ups-readerTxt">placeholder</p>    <div class="ups-text_wrapper ups-input_wrapper">        <input type="text"         	name="locationId"             class="ups-form_input"            id="ups-location_id_input"             maxlength="10"            aria-describedby="locationIdHelperTxt">    </div>    <!-- ups-text_wrapper ups-input_wrapper --></div><!-- End Location ID -->{{/if}}<!-- /////////////////////////////// --><!-- //// End CAC               //// --><!-- /////////////////////////////// -->