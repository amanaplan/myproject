<script type="text/javascript">
$(document).ready(function() { 
 var d = new Date();
 document.getElementById("currentDay").value=d.getDay();
 isHTMLCompatible();
 getCurrentTime();
 } );
function getLocation()
 {
 if (navigator.geolocation)
 {
 navigator.geolocation.getCurrentPosition(
 function(position) {
 document.getElementById("latitude").value=position.coords.latitude;
 document.getElementById("longitude").value=position.coords.longitude; 

 if(document.getElementById("latitude").value != "" && document.getElementById("longitude").value != ""){
 $('#loc_form').append('<input type="hidden" id="my_current_location" name="search_location" value="My Current Location"/>');
 document.forms["loc_form"].submit(); 
 } },
 function error(error) {
 showErrorMessage("We are currently unable to determine your GPS location. Please search for a UPS location by address instead.");
 },
 {
 timeout:15000
 }
 );
 }
 return false;
 }

function isHTMLCompatible()
{
 if (navigator.geolocation)
 {
 document.getElementById("hide_optional").style.display = 'block';
 }
}

 function getCurrentTime()
 {
 var d = new Date();
 var halfHour = Math.round(d.getMinutes()/30);
 if(halfHour == 0)
 {
 halfHour = '00';
 }else{
 halfHour = '30';
 }
 
 var hour = d.getHours();
 if("US" == "US"){
 var ampm = ' AM';
 if(hour > 12) 
 {
 ampm = ' PM';
 }
 
 hour = hour % 12;
 hour = hour ? hour : 12;
 } 
 hour = hour < 10 ? '0'+hour : hour;
 var finalString = hour.toString();
 finalString = finalString.concat(":");
 finalString = finalString.concat(halfHour.toString());
 if("US" == "US"){
 finalString = finalString.concat(ampm.toString());

 }
 document.getElementById("currentTime").value=finalString;
 } 
</script>





