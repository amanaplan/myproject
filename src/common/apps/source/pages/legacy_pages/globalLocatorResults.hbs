function callSaveLoc(){
  var isDuplicateNickname = "false";
  $('#nicknameempty').hide();
  $('#nicknameduplicate').hide();
  $('#broswernotsupported').hide();
  // check if the broswer supports cookies or if cookies are disabled
  var cookieEnabled = (navigator.cookieEnabled) ? true : false;
  if (typeof navigator.cookieEnabled == "undefined" && !cookieEnabled) { 
    document.cookie="testcookie";
    cookieEnabled = (document.cookie.indexOf("testcookie") != -1) ? true : false;
  }
  if(!cookieEnabled) {
    $('#broswernotsupported').show();
    return;
  }     
  var nickname = $('#saveTxt').val();
  var cookie = readCookie("GDOL_AutoSuggest")
  var obj = JSON.parse(cookie);
  var saved = obj.savedSearches;
  var decodedSavedName = '';
  if(nickname == ''){
    $('#nicknameempty').show();
  } else {
    for (var i in saved) {    
      decodedSavedName = decodeURIComponent(saved[i].nickName.replace(/\+/g, ' '));
      if (saved[i] !== null && saved[i].nickName !== null
          && saved[i].nickName.length > 0 &&  decodedSavedName == nickname) {
        $('#nicknameduplicate').show();
        isDuplicateNickname = "true";
        break;
      }     
    }
    if (isDuplicateNickname == 'false') {
      gdol_saveCookie();
    }
  }
}
