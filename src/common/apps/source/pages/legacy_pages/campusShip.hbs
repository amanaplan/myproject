<script type="text/javascript">
function highlightErrorFields() {
 var errorList = '';
 
 if (errorList.length == 0) {
 return;
 }
 
 var errorFieldNames = jQuery.parseJSON(errorList);
 if (errorFieldNames) {
 for (var i = 0; i < errorFieldNames.length; i++) {
 var selector = '[name="'+ errorFieldNames[i] +'"]:first';
 var errorFieldId = $(selector).attr("id");
 if (errorFieldId) {
 selector = 'label[for*="'+ errorFieldId +'"]:first';
 var errorFieldLabel = $(selector);
 errorFieldLabel.addClass("error");
 }
 }
 }
}
