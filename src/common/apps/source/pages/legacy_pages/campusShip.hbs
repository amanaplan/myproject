<script type="text/javascript">
	var select_shipTo = $("#select_shipTo"); 
	var hiddenAddressField_shipTo = $("#shipToAddressBookId");
	var allOptions = null;

	function eabSelected() {
		submitWithAOPair(document.mainPageForm,'EABSelected','CreateAShipment','');
	}
		
	function packageCountChanged_shipTo() {
		if (!allOptions){
			allOptions = $('#select_shipTo').children();
		}
		var selectedAddress = hiddenAddressField_shipTo.val(); 
		var packageCount = $("#packageCount");
		if (packageCount && packageCount.val) {			
			if ('CreateAShipment' === 'CreateAShipment' || 'CreateAShipment' === 'MobileAddress') {
				if (packageCount.val() === '1') {  
					$('#select_shipTo').html(allOptions.filter('*'));				
				} else {
					$('#select_shipTo').html(allOptions.filter('.shipto-opt'));				
				}
			}
			if ( selectedAddress === "" ) {				
				$("#select_shipTo option:first").prop('selected', true);				
			} else {
				$('#select_shipTo').val(selectedAddress);
			}
		}
	}
	
	function initializeDropdown_shipTo() {		
		if ('shipTo' === 'shipTo') {
			var packageCount = $("#packageCount");
			if (packageCount && packageCount.val) {			
				if (packageCount.val() != '1') {
					packageCountChanged_shipTo();
				}
			}
		}
		var selectedId_shipTo = hiddenAddressField_shipTo.val();
		if (selectedId_shipTo) select_shipTo.val(selectedId_shipTo);
	}  // initializeDropdown

	$(function() {
		initializeDropdown_shipTo();
		select_shipTo.change(function() {
			  var changed_shipTo = $("#select_shipTo"); 
			  var addressBookNickname = document.getElementById("addrBookNickname"); 
			  if ('shipTo' === 'shipTo' && addressBookNickname) {
				  var selectedOption = $("#select_shipTo > :selected");
				  var selectedText = selectedOption.text();
				  addressBookNickname.value = selectedText;
			  }
			  var changedId_shipTo = changed_shipTo.val();
			  if( jQuery.trim(changedId_shipTo).length > 0 ) {
			  	  hiddenAddressField_shipTo.val(changedId_shipTo);
				  nicknameSelected( 'shipTo', 'CreateAShipment' );
			  }
		});
		if ('shipTo' === 'shipTo' && 'CreateAShipment' === 'CreateAShipment') {
			var packageCount_shipTo = $("#packageCount");
			if (packageCount_shipTo) {
				packageCount_shipTo.change(function() {
					packageCountChanged_shipTo();
				});
			}
		}  // shipTo
