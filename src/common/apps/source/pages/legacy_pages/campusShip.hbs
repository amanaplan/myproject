<script language="javascript1.2" type="text/javascript">
var isReview = false;
var isTaxID = false;
var isMailInv = false;

function searchADLDropOff(uri) { 
 if (validateADL()) { 
 goToLSWidget(document.mainPageForm, uri, "EnterLSDataDropOff___CreateAShipment"); 
 }
}

function selectMailInvoice(hasTaxID) {
 if (hasTaxID) {
 $("#mailInvoiceCheckbox").prop("checked", true).prop("disabled", true);
 $("#mailInvoiceHidden").val(true);
 }
 else {
 $("#mailInvoiceCheckbox").prop("checked", false).prop("disabled", false);
 $("#mailInvoiceHidden").val(false);
 $("#payAtStoreTaxID").val('');
 }
}
function taxIdEntered() {
 if ($("#payAtStoreTaxID").val()) {
 $("#hasTaxIdCheckbox").prop("checked", true);
 selectMailInvoice(true);
 }
}

$("#hasTaxIdCheckbox").prop("checked", isTaxID);
$("#mailInvoiceCheckbox").prop("checked", isMailInv);
if (isReview) {
 $("#hasTaxIdCheckbox").prop("disabled", true);
 $("#mailInvoiceCheckbox").prop("disabled", true);
 $("#taxIdDL").hide();
}
if ($("#hasTaxIdCheckbox").prop("checked")) {
 $("#mailInvoiceCheckbox").prop("disabled", true);
