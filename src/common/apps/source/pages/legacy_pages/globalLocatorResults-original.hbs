function emailLocDialogSubmit() {
  try { 
    $('#locemailsubmit').attr('disabled','disabled');
    $('#loc_requirederror').hide();
    $('#loc_invaliderror').hide();
    $('#loc_emailnotsent').hide();
    $('#loc_maxemailerror').hide();
    var is_wwef = "false";
    var geolat=$('#geolat').val();
      var geolong=$('#geolong').val();
      var txtquery=$('#txtQuery').val();
      var loc=$('#loc').val();
      var country=$('#country').val();
      var Locations=$('#locationId').val();
      var locInfoEmails=$('#locemailloctxt').val().replace(/\s+/g, '');
      var locInfoEmailsList = locInfoEmails.split(',');
      var allEmailsNotValid = 0;
      var emailRegEx = /^[A-Z0-9._%+-]+[^.@]+@[A-Z0-9.-]+\.[A-Z]{2,4}$/i;
    // Check if all the email address are in correct format.
      for(var i=0;i<locInfoEmailsList.length;i++) {                   
             if (locInfoEmailsList[i].search(emailRegEx) == -1) {
              allEmailsNotValid = 1;  
            break;
             }
      } 
    
      var locDetail =  $.parseJSON($("#locationJson").val());
    for (var i = 0; i < locDetail.length; i++) {
      if (Locations === locDetail[i].locationId) {
        is_wwef = locDetail[i].frieghtIndicator;  
        break;
      }
    }   
    
      if(Boolean(allEmailsNotValid )) {       
         $('#loc_requirederror').show();
         $('#locemailsubmit').attr('disabled',false);
      } else if(locInfoEmailsList.length > 5) {
         $('#loc_maxemailerror').show();
         $('#locemailsubmit').attr('disabled',false);       
    } else {
        $.ajax({ 
           url: "/dropoff/emaillocation", 
           type:'POST', 
           dataType : 'json',
           data:{
              reqType:"loc_info_email",
            emailaddresses:locInfoEmails,
            loc:loc,
            country:country,
            geolat:geolat,
            geolong:geolong,
            Locations:Locations,
            is_wwef:is_wwef
             }, 
            success:function(data) 
            {
              getLocInfoNSStatus(data);
            }, 
            error:function(data) 
            {
              dcsMultiTrack('DCSext.pPID','locator/DOL/dro_emailinfo_dlg.html','DCS.dcsuri','/locator/DOL/dro_emailinfo_dlg.html','WT.dl','102','DCSext.WBPM_ce','');
              $('#loc_emailnotsent').show(); 
            } 
         });
      }   
  } catch (e) {}    
}

