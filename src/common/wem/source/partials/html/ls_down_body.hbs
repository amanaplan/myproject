<!-- @start ls_down_body -->
<!-- For full documentation see partial partials/html/ls_down_body.hbs -->
<div class="ups-page-title ups-wrap">
    <div class="ups-wrap_inner">
        <div class="page-title_table">
            <div class="page-title_cell">
                <h1 id="ups-page_title" tabindex="-1">{{header}}</h1>
            </div>
        </div>
    </div>
</div>  

<div class="ups-wrap ups-form_wrap">
	<div class="ups-wrap_inner">
		<div class="introductory">
			<p>{{p1}}</p>
			<p>{{p2}}</p>
		</div>
		<div class="ups-form_ctaGroup">
			<a href="{{track_url}}" class="ups-cta ups-cta_primary">{{track_label}}</a>
			<a href="{{ship_url}}" class="ups-cta ups-cta_primary">{{ship_label}}</a>
		</div>
	</div>
</div>
<!-- @end ls_down_body -->