		closeAtTop: false,
		currentText: "",
		clearText: "",
		minDate: new Date(),
		prevText: '',
		nextText: '',
		showOn:'focus'
	});
	$("#end_date").datepicker({
		duration:"",
		buttonImage: "/img/icn_calendar.gif",
		buttonText:"",
		changeMonth: false,
		changeYear: false,
		closeAtTop: false,
		currentText: "",
		clearText: "",
		minDate: new Date(),
		prevText: '',
		nextText: '',
		showOn:'focus'
	});
});
</script>
