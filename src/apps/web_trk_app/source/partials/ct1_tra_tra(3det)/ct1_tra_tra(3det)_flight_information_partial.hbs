
<div class="panel panel-default">
    <!-- Begin Section Header -->
    <div id="collapse2" class="panel-heading">
        <span class="btnlnkL collapseIcon" aria-hidden="true"></span>Flight Information
    </div>
    <!-- End Section Header -->
    <!-- Begin Section Body -->
    <div class="panel-body">
        {{#if freight_progress_information_flight_information}}
        	<table summary="" border="0" cellpadding="0" cellspacing="0" class="dataTable">
        		<tr>
        			<th scope="col">status</th>
        			<th scope="col">flight</th>
        			<th scope="col" class="full">description</th>
        			<th scope="col">departure</th>
        			<th scope="col">arrival</th>
        		</tr>
        		<tr valign="top" class="odd">
        			<td class="nowrap">status</td>
        			<td class="nowrap">flight #</td>
        			<td>description</td>
        			<td class="nowrap">mm/dd/yyyy<br>
        				hh.mm a.m.</td>
        			<td class="nowrap">mm/dd/yyyy<br>
        				hh.mm a.m.</td>
        		</tr>
        		<tr valign="top">
        			<td class="nowrap">status</td>
        			<td class="nowrap">flight #</td>
        			<td>description</td>
        			<td class="nowrap">mm/dd/yyyy<br>
        				hh.mm a.m.</td>
        			<td class="nowrap">mm/dd/yyyy<br>
        				hh.mm a.m.</td>
        		</tr>
        	</table>
        {{/if}}
        {{#if freight_progress_information_voyage_information}}
        	<table summary="" border="0" cellpadding="0" cellspacing="0" class="dataTable">
    			<tr>
    				<th scope="col">status</th>
    				<th scope="col">vessel</th>
    				<th scope="col">voyage</th>
    				<th scope="col" class="full">description</th>
    				<th scope="col">departure</th>
    				<th scope="col">arrival</th>
    			</tr>
    			<tr valign="top" class="odd">
    				<td class="nowrap">status</td>
    				<td class="nowrap">vessel name</td>
    				<td class="nowrap">voyage #</td>
    				<td>description</td>
    				<td class="nowrap">mm/dd/yyyy<br>
    					hh.mm a.m.</td>
    				<td class="nowrap">mm/dd/yyyy<br>
    					hh.mm a.m.</td>
    			</tr>
    			<tr valign="top">
    				<td class="nowrap">status</td>
    				<td class="nowrap">vessel name</td>
    				<td class="nowrap">voyage #</td>
    				<td>description</td>
    				<td class="nowrap">mm/dd/yyyy<br>
    					hh.mm a.m.</td>
    				<td class="nowrap">mm/dd/yyyy<br>
    					hh.mm a.m.</td>
    			</tr>
    		</table>
        {{/if}}
    </div>
    <!-- End Section Body -->
</div>
