                    </ul>
                </div>
                <!-- End Application Header -->

                <!-- Begin Application Body -->
                <div class="appBody clearfix">

                    <p>information about the products added to your shipment is displayed below. you must have at least one product in your shipment, and a maximum of 50.</p>

                    <!-- ////////////////////////////////////////// -->
                    <!-- Begin Section Module - Products Added Summary -->
                    <div class="secLvl secLvlPlain clearfix">
                    <fieldset>

                        <!-- Begin Section Header -->
                        <div class="secHead clearfix">
