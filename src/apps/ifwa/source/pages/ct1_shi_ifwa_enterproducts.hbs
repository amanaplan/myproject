                        <div class="secBody">

                            <div class="clearfix">


                                <!-- Begin Default - No Products -->
                                <table summary="repl_summary" title="repl_title" class="dataTable full">
                                <!--<caption>repl_caption</caption>-->
                                    <thead>
                                        <tr>
                                            <th>product name</th>
                                            <th>product description</th>
                                            <th>unit of measure</th>
                                            <th>number of units</th>
                                            <th>price per unit</th>
                                        </tr>
                                    </thead>

                                    <tbody>
                                        <tr>
