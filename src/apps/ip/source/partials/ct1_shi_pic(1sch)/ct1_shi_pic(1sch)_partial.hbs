<script type="text/javascript">
var htmlcobrand;
$(document).ready(function(){
	/* Prepend Control Box */
	$('body').prepend('<div id="control_box" class="ctrl_box"><div class="control_box_head"><label class="error" id="">Switch View:</label></div><div class="alignCenter"><input type="button" onclick="switchContainer(\'ccacmpad1\')" value="Campus Pickup" class="btn"><br><input type="button" onclick="switchContainer(\'ccCntMod\')" value="Internet Pickup" class="btn"></div></div>');
	/* Set default container and stylesheet to Internet Pickup */
	$('.switchContainer').removeClass('ccacmpad1');
	$('.switchContainer').attr('id', 'ccCntMod');
	//chooseStyle('Internet Pickup', 60);	
	htmlcobrand = $('.cobrand').html();
	$('.cobrand').remove();
	
});
/* function to switch views */
function switchContainer(container) {
	if(container == 'ccCntMod') {
		$('.switchContainer').removeClass('ccacmpad1');
		$('.switchContainer').attr('id', container);
		//chooseStyle('Internet Pickup', 60);
		$('.cobrand').remove();
	} 
	if(container == 'ccacmpad1') {
		$('.switchContainer').attr('id', '');
		$('.switchContainer').addClass(container);
		//chooseStyle('Campus Pickup', 60);
		$('.cobrand').remove();
		$('form[action=repl_fname]').append($('<div class="cobrand" align="center" />').append(htmlcobrand));
	}
}

</script> 

<!------------- -- --------------> 
