<div class="secLvl step">
                <div class="secHead clearfix">
                    <div class="stepNumberDiv"><span class="stepNumber">1</span></div>
                    <h3>{{#unless shipping_label.no-printed }}Shipping Label Questions{{/unless}}{{#if shipping_label.no-printed }}Do you have UPS Shipping Labels?{{/if}}</h3>
                </div>
                <!-- End Header Bar --> 
                
                <div class="secBody clearfix">
                    <fieldset class="ups-form_group">
						<legend class="ups-form_label">Do you have pre-printed <a href="repl_href" class="btnlnkR helpIconR">UPS Shipping Labels</a> for your {{#unless shipping_label.no-printed }}shipment?{{/unless}}{{#if shipping_label.no-printed }}packages or letters?{{/if}}</legend>
						<div class="ups-buttonList_wrapper ups-input_wrapper">
							<input id="yesShippingLabels" class="ups-radio-custom" name="shippingLabelsQuestion" value="1" type="radio">
							<label for="yesShippingLabels" class="ups-radio-custom-label">Yes</label>
						</div>
						<div class="ups-buttonList_wrapper ups-input_wrapper">
							<input id="noShippingLabels" class="ups-radio-custom" name="shippingLabelsQuestion" value="2" type="radio">
							<label for="noShippingLabels" class="ups-radio-custom-label">No</label>
						</div>
					</fieldset>
                    {{#unless shipping_label.no-printed }}
					<div class="ups-form_group">
						<label class="ups-form_label" for="trackNumbers">Optional: Enter tracking numbers, separated by commas, to check if pickup charges are prepaid.</label>
						<p><em><strong>(If you have <a href="repl_href"  class="btnlnkR helpIconR"> return label</a> tracking numbers, enter them here.)</strong></em></p>							
						<div class="row">
							<div class="col-xs-5">
								<div class="ups-form_group">
									<div class="ups-textArea_wrapper ups-input_wrapper">
										<textarea class="ups-textArea" cols="32" id="trackNums" name="trackNums" rows="4"></textarea>
										<p><em><strong>(Maximum 30 tracking numbers)</strong></em></p>
									</div>
								</div>
							</div>
						</div>
						<p class="error">Note: Pickup charges will apply for the tracking number(s) entered. To avoid charges, you may drop your package at a nearby location.</p>
					</div>
                    {{/unless}}
					
					{{#if shipping_label.no-printed }}
					<p><strong>You selected no pre-printed labels.</strong></p>
					<div class="indent secLvlPad">
