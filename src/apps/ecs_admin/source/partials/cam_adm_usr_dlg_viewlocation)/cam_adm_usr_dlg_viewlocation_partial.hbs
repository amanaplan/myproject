            <div class="seccol9 group marginEnd">


                    <dl class="group">
                        <dt><label for="">location contact name:</label></dt>
                            <dd>{location data}</dd>

                        <dt><label for="" class="sel">e-mail:</label></dt>
                            <dd>{location data}</dd>
                    </dl>


                    <dl class="flexFirst group clearfix">
                        <dt><label for="">telephone:</label></dt>
                        <dd>{location data}</dd>
                    </dl>

                    <dl class="flexAfter group clearfix">
                        <dt><label for="">ext.</label></dt>
                            <dd>{0000}</dd>
                    </dl>

                    <div class="clear"></div>

                    <dl>
                    	<dt><label for="" class="sel">fax:</label></dt>
                        <dd>{location data}</dd>
                     </dl>

                    <!-- Begin Sec Module -->
                    <div class="secLvl secLvlPlain clearfix">

                        <!-- Begin Sec Header -->
                        <div class="secHead clearfix">
