                        </div>
                        <!-- End Sec Header -->
                        <!-- Begin Sec Body -->
                        <div class="secBody">

                            <dl class="group">
                                <dt><label for="" class="sel">support e-mail address:</label></dt>
                                <dd>{location data}</dd>
                            </dl>

                            <dl class="flexFirst group clearfix">
                                <dt><label for="">support phone:</label></dt>
                                    <dd>{location data}</dd>
                            </dl>
                            <dl class="flexAfter group clearfix">
                                <dt><label for="">ext.</label></dt>
                                    <dd>{0000}</dd>
                            </dl>




                        </div>
                        <!-- End Sec Body -->


                    </div>
                    <!-- End Sec Module -->






            </div>
        <!-- End Column -->

    <div class="clear"></div>

    <!-- Begin Column -->
            <div class="seccol9 marginBegin">


                <dl>
                    <dt><span class="label">location account number:</span></dt>
                    <dd>{location data}</dd>
                </dl>

				<dl>
                    <dt><span class="label">air freight 9 digit account number:</span></dt>
                    <dd>{9 digit air account data}</dd>
                </dl>

                <dl>
                    <dt><span class="label">number of users assigned to this location:</span></dt>
                    <dd>{0000}</dd>
                </dl>

                <dl>
                    <dt><span class="label">associated address group:</span></dt>
                    <dd>{location data}</dd>
                </dl>

            </div>
        <!-- End Column -->

