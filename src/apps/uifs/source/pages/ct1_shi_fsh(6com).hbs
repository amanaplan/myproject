    <div id="select_pnl_2">
        <dl role="presentation">
            <dt>
                <label for="dyn_pnl_select_2">View/Print</label>
            </dt>
            <dd>
                <select id="dyn_pnl_select_2" class="formcol5">
                    <option value="1">1 Non-Paperless</option>
                    <option value="2">2 Paperless</option>
                </select>
            </dd>
        </dl>
    </div>
</div>
