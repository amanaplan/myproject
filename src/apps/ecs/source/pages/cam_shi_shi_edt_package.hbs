---
title: create shipment page title
layout: legacy.hbs
version: v4
appID: com.ups.ECS
appCSS: ups.legacy_overrideECS
rightRail: true
dir: ltr
lang: en
---

<!-- **************************** -->
<!-- Begin Application Owned HTML -->
<!-- **************************** -->
<div id="main">
       
          <form action="repl_fname">
            
            <!-- Begin SubTabs -->
            <ul class="subTab clearfix">
              <li class="tab-on">Package</li>
              <li class="tab-off"><a href="repl_href">Freight</a></li>
            </ul>
            <!-- End SubTabs --> 
            
            <!-- USE Class="tab" for tab application display or use class="mod" for default application display --> 
            <!-- ECS no tabs, UIS Tabs --> 
            <!-- Begin Application Module -->
            <div class="appLvl col18 tab"> 
              
              <!-- Begin Application Header -->
              <div class="appHead clearfix">
                <h2 id="repl_id">edit information for all packages application header</h2>
                <ul>
                  <li><a id="repl_id" href="repl_href" class="btnlnkR helpiconR">help link</a></li>
                </ul>
              </div>
              <!-- End Application Header --> 
              
              <!-- Begin Application Body -->
              <div class="appBody clearfix">
                <p class="error">&gt;&gt;&gt;error message.</p>
                <p>please enter your package information below paragraph text. required fields are indicated with<img alt="" border="0" height="12" width="11" src="/img/1.gif" class="requiredIcon" title="Required">.</p>
                <dl class="outHozFlex clearfix">
                  <dt>total packages label:</dt>
                  <dd id="repl_id">n</dd>
                </dl>
                
                <!-- Begin Section Module -->
                <div class="secLvl step clearfix">
                  <fieldset>
                    <!-- Begin Section Header -->
                    <div class="secHead clearfix">
                      <h3 id="repl_id" class="">package n</h3>
                    </div>
                    <!-- End Section Header --> 
                    
                    <!-- Begin Section Body -->
                    <div class="secBody clearfix">
                      <dl class="group">
                        <dt>
                          <label for="repl_id"><a id="repl_id" href="repl_href" class="btnlnkR helpIconR">package type label:</a></label>
                        </dt>
                        <dd>
                          <select name="" id="repl_id">
                            <option value="" selected>{selected package}</option>
                            <option value="">my packaging</option>
                          </select>
                          <img alt="" border="0" height="12" width="11" src="/img/1.gif" class="requiredIcon" title="Required"></dd>
                        <dt>
                          <label id="repl_id" for="repl_id">weight label:</label>
                        </dt>
                        <dd>
                          <input name="" type="text" id="repl_id" value="{10.0}" size="5">
                          lbs<img alt="" border="0" height="12" width="11" src="/img/1.gif" class="requiredIcon" title="Required"></dd>
                      </dl>
                      <label id="repl_id" for="repl_id"><a id="repl_id" href="repl_href" class="btnlnkR helpIconR">package dimensions:</a></label>
                      <fieldset>
                        <dl class="flexFirst group">
                          <dt>
                            <label id="repl_id" for="repl_id">length label:</label>
                          </dt>
                          <dd>
                            <input name="" type="text" id="repl_id" value="{lenght}" size="5">
                            x</dd>
                        </dl>
                        <dl class="flexAfter group">
                          <dt>
                            <label id="repl_id" for="repl_id">width label:</label>
                          </dt>
                          <dd>
                            <input name="" type="text" id="repl_id" value="{width}" size="5">
                            x</dd>
                        </dl>
                        <dl class="flexAfter group">
                          <dt>
                            <label id="repl_id" for="repl_id">height label:</label>
                          </dt>
                          <dd>
                            <input name="" type="text" id="repl_id" value="{height}" size="5">
                            [in.]</dd>
                        </dl>
                      </fieldset>
                      
                      <!-- ---------------- --> 
                      <!--  -UIS ADDITION- --> 
                      <!-- ---------------- -->
                      <p class="group">
                        <label id="repl_id" for="repl_id"><a id="repl_id" href="repl_href" class="btnlnkR helpIconR">large or unusually shaped packages label</a></label>
                      </p>
                      <!-- ---------------- --> 
                      <!-- END UIS ADDITION --> 
                      <!-- ---------------- -->
                      
                      <table border="0" cellspacing="0" cellpadding="0">
                        <tr>
                          <td class="sel"><input name="" type="checkbox" value="" id="repl_id"></td>
                          <td><label id="repl_id" for="repl_id" class="sel">large package: text.</label></td>
                        </tr>
                        <tr>
                          <td class="sel"><input name="" type="checkbox" value="" id="repl_id"></td>
                          <td><label id="repl_id" for="repl_id" class="sel">additional landing: text.</label></td>
                        </tr>
                      </table>
                      <dl>
                        <dt>
                          <label id="repl_id" for="repl_id"><a id="repl_id" href="repl_href" class="btnlnkR helpIconR">declared value label:</a></label>
                        </dt>
                        <dd>
                          <input name="" type="text" id="repl_id" value="{nn.00}" size="5">
                          usd</dd>
                      </dl>
                      <p><strong>note:</strong> additional shipping fees.</p>
                      <dl>
                        <dt>
                          <label id="repl_id" for="repl_id"><a id="repl_id" href="repl_href" class="btnlnkR helpIconR">merchandise description label:</a></label>
                        </dt>
                        <dd>
                          <input name="" type="text" id="repl_id" value="{example description}">
                          <img alt="" border="0" height="12" width="11" src="/img/1.gif" class="requiredIcon" title="Required"></dd>
                      </dl>
                      <label id="repl_id" for="repl_id"><a id="repl_id" href="repl_href" class="btnlnkR helpIconR">references label:</a></label>
                      
                      <!-- ---------------- --> 
                      <!--  -UIS ADDITION- --> 
                      <!-- ---------------- -->
                      <p>ups gives you the option to track your shipments using <a id="repl_id" href="repl_href" class="btnlnkR helpIconR">references link</a> that you define paragraph.</p>
                      <!-- ---------------- --> 
                      <!-- END UIS ADDITION --> 
                      <!-- ---------------- -->
                      
                      <fieldset>
                        <dl class="group">
                          <!-- add flexFirst for UIS -->
                          <dt>
                            <label id="repl_id" for="repl_id">{reference #1 label}:</label>
                          </dt>
                          <dd>
                            <input name="" type="text" id="repl_id" value="{reference 1}">
                            <a id="repl_id" href="repl_href">serach link</a></dd>
                        </dl>
                        <dl class="group">
                          <!-- add flexAfter for UIS -->
                          <dt>
                            <label id="repl_id" for="repl_id">{reference #2 label}:</label>
                          </dt>
                          <dd>
                            <input name="" type="text" id="repl_id" value="{reference 2}">
                            <a id="repl_id" href="repl_href">serach link</a></dd>
                        </dl>
                        <dl class="group">
                          <!-- add flexAfter for UIS -->
                          <dt>
                            <label id="repl_id" for="repl_id">{reference #3 label}:</label>
                          </dt>
                          <dd>
                            <input name="" type="text" id="repl_id" value="{reference 3}">
                            <a id="repl_id" href="repl_href">serach link</a></dd>
                        </dl>
                      </fieldset>
                      <table border="0" cellspacing="0" cellpadding="0">
                        <tr>
                          <td class="sel"><input name="" type="checkbox" id="repl_id" value=""></td>
                          <td><label id="repl_id" for="repl_id" class="sel"><a id="repl_id" href="repl_href" class="btnlnkR helpIconR">add a bar code for reference #1 (applies all packages)</a>.</label></td>
                        </tr>
                      </table>
                      <div class="hr"><img alt="" border="0" height="1" src="/img/1.gif" width="1"></div>
                      <h4 id="repl_id">additional shipping options and services header</h4>
                      <label id="repl_id" for="repl_id"><a id="repl_id" href="repl_href" class="btnlnkR helpIconR">proactive response label:</a></label>
                      <br>
                      <table border="0" cellspacing="0" cellpadding="0">
                        <tr>
                          <td class="sel"><input name="" type="radio" id="repl_id" value=""></td>
                          <td><label id="repl_id" for="repl_id" class="sel">proactive response requested</label>
                            <br>
                            <input name="" type="checkbox" id="repl_id" value="">
                            <label id="repl_id" for="repl_id" class="sel">refrigeration needed</label></td>
                        </tr>
                        <tr>
                          <td class="sel"><input name="" type="radio" id="repl_id" value=""></td>
                          <td><label id="repl_id" for="repl_id" class="sel">proactive response not requested</label></td>
                        </tr>
                      </table>
                      
                      <!-- ---------------------------- --> 
                      <!--  -Delivery Confirmation UIS- --> 
                      <!-- ---------------------------- -->
                      <p>select the delivery options and/or confirmation method.</p>
                      <dl>
                        <dt>
                          <label id="repl_id" for="repl_id"><a id="repl_id" href="repl_href" class="btnlnkR helpIconR">delivery options label:</a></label>
                        </dt>
                        <dd>
                          <select name="" id="repl_id">
                            <option value="" selected>{selected option}</option>
                          </select>
                        </dd>
                      </dl>
                      <!-- ---------------------------- --> 
                      <!--  -END Delivery Confirmation UIS- --> 
                      <!-- ---------------------------- -->
                      
                      <table border="0" cellpadding="0" cellspacing="0">
                        <tr>
                          <td class="sel"><input type="checkbox" value="repl_value" name="repl_name1" id="repl_id1"></td>
                          <td><label for="repl_id1" class="sel"><a href="repl_href" class="btnlnkR helpIconR">ups premium care</a></label></td>
                        </tr>
                      </table>
                    </div>
                    
                    <!-- End Section Body --> 
                    
                    <!-- Begin Section Footer --><!-- End Section Footer -->
                  </fieldset>
                </div>
                <!-- End Section Module -->
                
                <p><a id="repl_id" href="repl_href" class="btnlnkL plusMini">add one link text</a></p>
              </div>
              <!-- End Application Body --> 
              <!-- Begin Application Footer -->
              <div class="appFooter"> 
                <!-- Begin Button Bar Row -->
                <div class="btnBar">
                  <input name="" type="button" id="repl_id" value="cancel" class="btn">
                  <input name="" type="submit" id="repl_id" value="next" class="btnArw">
                </div>
                <!-- End Button Bar Row --> 
              </div>
              <!-- End Application Footer --> 
            </div>
            <!-- End Application Module -->
            
            <div align="center" class="cobrand"><br>
              Company Support: &nbsp;&nbsp; 678-585-1321 &nbsp;&nbsp;<a href="mailto:test@ups.com">test@ups.com</a></div>
          </form>
        </div>
        <!-- /applicationArea --> 