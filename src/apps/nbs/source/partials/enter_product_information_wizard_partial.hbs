                {{#if error}}
                	{{> A03_page_level_error this.epiw_si_error }}
                {{/if}}
                <p>Detail the contents of your shipment one item at a time.</p>
                <p class="ups-form_subheader">Step 1 of 2</p>
                <div class="ups-section">
                    <div class="row">
                        <div class="col-md-5">
                            <div class="ups-form_group ups-form_required">
                                <label for="siToCountryDropdown" class="ups-no_label">Select Item <span aria-hidden="true" class="ups-required_indicator"></span> </label>
                                <div class="ups-dropdown_wrapper ups-input_wrapper">
                                    <select id="siToCountryDropdown" name="siToCountryDropdown" class="ups-dropdown{{#if error}} ng-invalid ng-dirty{{/if}}" {{#if error}}aria-describedby="siToCountryDropdown_error"{{/if}} aria-required="true" required>
                                        <option>Select one</option>
                                    </select>
                                </div>
                              {{#if non_error}}
                              <div class="ups-check ups-active">
                                      <span class="push icon ups-icon-check" aria-hidden="true"></span>
                                      <span class="ups-readerTxt">Check</span>
                                  </div>
                              {{/if}}
                              {{#if error}}
                                {{>A11_field_lvl_error error_siToCountryDropdown }}
