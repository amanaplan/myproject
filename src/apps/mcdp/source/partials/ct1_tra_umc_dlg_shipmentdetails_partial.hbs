                    <button value="" type="submit" class="button btnGroup7 btnGradient btnGradient7 arrowIconRight formcol7 clearfix" style="background-position: 0pt -2px;">driver instructions</button>
                </div>
            </div>
            <!-- End Section Body --> 
            {{/if}}
        </div>
        <!-- End Section Module --> 
    
    {{#unless tooltip}}
    </div>
    <div class="appFooter">
        <div class="btnBar alignCenter">
            <input name="" type="button" value="close" class="btn">
