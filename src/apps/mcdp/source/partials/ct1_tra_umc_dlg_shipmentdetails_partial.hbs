                {{#unless tooltip}}
                <div class="subcol13 marginBegin">
                {{/unless}}
                    <dl class="outHozFixed outHozFixedCol5 clearfix" role="presentation">
                        {{#each list_item}}
                            <dt> <span class="label">{{label}}</span> </dt>
                            <dd class="formcol8">
                                {{#if link}}
                                    <a href="repl_href" class="btnlnkR helpIconR">{{link_text}}</a>
                                {{/if}}
                                
                                {{{title_text}}}
                                
                                {{#if copy_link}}
                                	Import Fees {{#if paid}}Paid{{else}}Due{{/if}} 
                                    <a href="repl_href" class="btnlnkR helpIconR">{{#if paid}}View Details{{else}}Pay Now{{/if}}</a>
                                {{/if}}
                                
                                {{{copy}}}
                                
                                {{#if address}}
                                	<br/>1234 Main St<br/>Roswell, GA, 30076 US
                                {{/if}}
                                
                                {{#if phone}}
                                	<br/>Tel: 555-123-4567
                                {{/if}}
                                
                                {{#if email}}
                                	<br/>store@email.com
                                {{/if}}
                                
                                {{#if sig_requ_link}}
                                	<br/>Signature Required
                                    <br/><a href="repl_href">Authorize Shipment Release</a>
                                {{/if}}
                                
								{{{dd_note}}}
                               
                            </dd>
                        {{/each}}
                    </dl>
                  	<div class="clear"></div>
                
                {{#unless tooltip}}
                </div>
                {{/unless}}
                {{#unless no_btn}}
