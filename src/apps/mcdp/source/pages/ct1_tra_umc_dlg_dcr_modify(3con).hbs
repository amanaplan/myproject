---
title: Address Modification
layout: legacy.hbs
version: v4
appID: com.ups.mcdp
appCSS: ups.legacy_overrideMCDP
dir: ltr
lang: en
---


<div id="main">
	{{> background_modal_content }}

<div id="HPDCR_Dialog" class="appLvl mod modal dialog">
  <div class="appHead clearfix">
    <h2>{{title}}</h2>
    <ul>
      <li>
        <input type="button" class="btnlnkL closeIcon" title="Close" id="modalClose">
      </li>
    </ul>
  </div>
  <div class="appBody clearfix">
    
    <!-- Begin New Delivery Address Information Module -->
    <div class="secLvl secLvlPlain clearfix">
      
      <!-- Begin Section Body -->
      <div class="secBody">
        <p>Thank you. Your address has been updated for this package. UPS will deliver the package to the address shown below. If there are additional packages in the same shipment, you must correct the address on each of them.</p>
        <p>Note: It may take a few moments before you can take further actions on your package.</p>
        <div class="group clearfix">
          <dl class="outHozFixed clearfix">
            <dt>
              <label for="repl_id">Tracking Number:</label>
            </dt>
            <dd id="repl_id">1Z12345678901234456</dd>
          </dl>
          <dl class="outHozFixed clearfix">
            <dt>
              <label for="repl_id">Ship To:</label>
            </dt>
            <dd id="repl_id">11270 Marvin Miller Dr.<br>
              Atlanta, GA, 30336	 US</dd>
          </dl>
        </div>
      </div>
      <!-- End Section Body -->
    </div>
    <!-- End New Delivery Address Information Module -->
    
  </div>
  <!-- End Application Body -->
  
  <!-- Begin Application Footer -->
  <div class="appFooter">
    <!-- Begin Button Bar Row -->
    <div class="btnBar alignCenter">
      <input name="" type="button" value="Close" class="btn">
    </div>
    <!-- End Button Bar Row -->
  </div>
  <!-- End Application Footer -->
  
</div>
<script type="text/javascript">

$(document).ready(function(){
	var modal=$('.modal').dialog({modal:true,bgiframe:true,dialogClass:'modal',width:710});
	$('#modalClose').click(function(e){
			modal.dialog('close');
	});
});


</script>
</div>
