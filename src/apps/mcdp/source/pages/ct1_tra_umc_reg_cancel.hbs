---
title: cancel ups my choice enrollment?
layout: legacy.hbs
version: v4
appID: com.ups.mcdp
appCSS: ups.legacy_overrideMCDP
dir: ltr
lang: en
---


<div id="main">
	{{> background_modal_content }}

<div class="appLvl mod modal dialog">
  <div class="appHead clearfix">
    <h2>{{title}}</h2>
    <ul>
      <li>
        <input id="modalClose" type="button" title="Close" class="btnlnkL closeIcon">
      </li>
    </ul>
  </div>
  <div class="appBody">
    <p>Are you sure you want to cancel?  Select <strong>Yes</strong> to cancel.  Select <strong>No</strong> to continue.</p>
  </div>
  <div class="appFooter">
    <div class="btnBar alignCenter">
      <input name="" type="button" value="yes" class="btn">
      <input name="" type="button" value="no" class="btn">
    </div>
  </div>
</div>

<script type="text/javascript">
		
$(document).ready(function(){

	$('.modal').dialog({modal:true,dialogClass:'modal', width:470});
	$('#modalClose').click(function(e){
		$('.modal').dialog('close');
	});	
});

</script>
	
</div>
