<script type="text/javascript">
$(document).ready(function(){
	var maxHeight = 518; // maximum modal height as defined and confirmed by David Davila
	var modalBaseHeight = $('#modal').height() - $('#acc_menu').height(); // height of the modal minus the accordion which will vary
	$('.interior_drop_anchor').click(function(){ // interior checkbox lists
		$(this).parents('dl').find('dt').not($(this).parent('dt')).next('.showHideDD').hide();
		$(this).parent('dt').next('.showHideDD').toggle();
		$(this).parents('dl').find('dt').not($(this).parent('dt')).find('.interior_drop_arrow').removeClass("interior_selected");
		$(this).parent('dt').find('.interior_drop_arrow').toggleClass("interior_selected");
	}).parent('dt').next('.showHideDD').hide();
	$('#acc_menu > .acc_handle').click(function(){ // the order of events below is important
		$('.interior_selected').removeClass("interior_selected");
		$('.showHideDD:visible').hide();
		$('#acc_menu > .acc_handle').not($(this)).removeClass("selected").next().hide();
		$(this).toggleClass("selected").next().toggle();
		$('#acc_menu').css("height","auto");
		if($('#acc_menu').height() + modalBaseHeight > maxHeight){
			$('#acc_menu').css("height",maxHeight - modalBaseHeight);
		}
	}).next().hide();
	$('.interior_header_cb').click(function(){ // Checkbox from the interior drop list anchor
		$(this).parent('dt').next('.showHideDD').find('.cb').attr("checked",$(this).attr("checked"));
	});
	$('.selectNoneMasse').click(function(){ // '{clear all link text} Filters' link
		$('.cb').removeAttr("checked");
	});
	$('.selectAll').click(function(){ // '{select all link text}' panel links
		$(this).parents('.acc_content').find('.cb').attr("checked","true");
	});
	$('.selectNone').click(function(){ // '{clear all link text}' panel links
		$(this).parents('.acc_content').find('.cb').removeAttr("checked");
	});
	$('.selectNoneMasse, .selectAll, .selectNone, .cb').bind("click", updateDisplay); // update the accordion header bar status when these are clicked
	updateDisplay();
});
function updateDisplay(){
	$('.acc_handle').each(function(i){ // set each header bar status to none, all, some, or error (error should be impossible)
		isck = false;
		ntck = false;
		theElement = this;
		if($(theElement).next('.acc_content').find('.cb').length > 0){
			$(theElement).next('.acc_content').find('.cb').each(function(n){
				this.checked ? isck = true : ntck = true;
			});
			if(isck){
				$(theElement).find('.filterStatus').text(ntck ? "({some selected text})" : "({all selected text})");
			} else {
				$(theElement).find('.filterStatus').text("({none selected text})");
			}
		}
	});
	$('.interiorCBStatus').each(function(i){
		isck = false;
		ntck = false;
		theElement = this;
		if($(theElement).parent('dt').next('.showHideDD').find('.cb').length > 0){
			$(theElement).parent('dt').next('.showHideDD').find('.cb').each(function(n){
				this.checked ? isck = true : ntck = true;
			});
			if(isck){
				$(theElement).text(ntck ? "(Some selected)" : "(All selected)");
				if(ntck){
					$(theElement).siblings('.interior_header_cb').attr("checked","true");
				}
			} else {
				$(theElement).text("");
				if(ntck){
					$(theElement).siblings('.interior_header_cb').removeAttr("checked");
				}
			}
		}
	});
}
</script> 
