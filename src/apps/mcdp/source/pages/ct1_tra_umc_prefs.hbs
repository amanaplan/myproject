<!-- SDC Tooltip Script --> 
<script type="text/javascript">
// set the persist behavior for the map legend infotip (causes the infotip to be added as a child of the event target)
var hoverPersist = true;
// set the space between the map legend items and the infotip
var spacer = -2;
$(document).ready(function(){
	$('#searchResultsDiv').css("height",$('#searchResultsDiv dl.upperThree').height());
	// hiding the closed panels on via JavaScript instead of through CSS class assignment
	$('.secLvl .sliderPanelHeader:not(.openPanelHeader)').siblings('.secBody, .secFooter').hide();
	// the 3 panel behaviors for the primary interactions
	$('.sliderPanelHeader').click(function(el){
		if(!$(this).hasClass('openPanelHeader')){
			$showSpeed = $(this).siblings('.secBody, .secFooter').height();
			$showSpeed = $showSpeed > 500 ? 500 : $showSpeed < 100 ? 250 : $showSpeed;
			$hideSpeed = $('.openPanelHeader').siblings('.secBody, .secFooter').height();
			$hideSpeed = $hideSpeed > 500 ? 500 : $hideSpeed < 100 ? 250 : $hideSpeed;
			// UNCOMMENT THE NEXT TWO LINES FOR TRUE ACCORDION
			//$('.sliderPanelHeader').removeClass('openPanelHeader');
			//$('.sliderPanelContent').hide($hideSpeed);
			$(this).addClass('openPanelHeader').siblings('.secBody, .secFooter').show($showSpeed);
		} // REMOVE THE "else" BLOCK BELOW FOR TRUE ACCORDION
			else {
			$hideSpeed = $(this).siblings('.secBody, .secFooter').height();
			$hideSpeed = $hideSpeed > 500 ? 500 : $hideSpeed < 100 ? 250 : $hideSpeed;
			$(this).removeClass('openPanelHeader');
			$(this).siblings('.secBody, .secFooter').hide($hideSpeed);
		}
	});
	// the highlighting for the items in the results list
	$('.resultsDiv li').click(function(){
		if(!$(this).hasClass('selected')){
			$(this).parents('.resultsDiv').find('li.selected').removeClass('selected');
			$(this).addClass('selected');
		}
	});
	// the show/hide behavior for the checkbox lists
	$('.showHideDT input').click(function(el){
		$(this).parent('dt').next('dd').toggle();
		$(this).toggleClass("minusMini");
	});
	// the infotip boxes for the map legend
	$('.mapLegend dd').hover(
		function(el){
			var targetOffset = $(this).offset();
			var targetWidth = $(this).width();
			var pokeFactor = targetOffset.left + $('#legendInfotip').width() - $(window).width();
			pokeFactor -= pokeFactor > 0 ? 0 : pokeFactor;

			if(hoverPersist){
				$(this).prepend($('#legendInfotip'));
				$('#legendInfotipDropSpace').css({left: pokeFactor,width: $(this).outerWidth()});
				spacer = 3;
				$('#legendInfotipBody').css("padding-bottom","0");
			}

			$('#legendInfotip').css({
				left: targetOffset.left-pokeFactor-2,
				top: targetOffset.top-$('#legendInfotip').height()-$(this).outerHeight()/2 + spacer,
				display: "block"
			});
			$('#legendInfotipPointer').css("left",(targetWidth/2));
		},
		function(el){
			$('#legendInfotip').hide();
		}
	);
});
</script> 
